[core]
	repositoryformatversion = 0
	filemode = false
	bare = true
	symlinks = false
	ignorecase = true
	hideDotFiles = dotGitOnly
	sharedrepository = 2
[receive]
	denyNonFastforwards = true
[remote "Codene"]
	url = git@github.com:mllarin/ar-ml-coden.git
	fetch = +refs/heads/*:refs/remotes/Codene/*
